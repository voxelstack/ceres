    "license": "MIT",
    "author": "voxelstack <voxelstack@gmail.com>",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/voxelstack/ceres.git"
    },
    "type": "module",
    "main": "dist/lib/index.js",
    "types": "dist/lib/index.d.ts",
    "scripts": {
        "compile:ts": "tsc --project ./tsconfig.json",
        "compile:ts:watch": "tsc --watch --project ./tsconfig.json",
        "compile:web": "node ./esbuild.js",
        "compile:web:watch": "node ./esbuild.js --watch",
        "compile": "run-p compile:ts compile:web",
        "test": "cross-env TSX_TSCONFIG_PATH=./test/tsconfig.json nyc mocha",
        "test:watch": "cross-env TSX_TSCONFIG_PATH=./test/tsconfig.json nyc mocha --watch",
        "format": "prettier . --check",
        "lint": "eslint ./src ./test"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/chai": "^5.2.3",
        "@types/chai-dom": "^1.11.3",
        "@types/jsdom": "^27.0.0",
        "@types/jsdom-global": "^3",
        "@types/mocha": "^10.0.10",
        "@types/node": "^25.0.3",
        "@types/sinon": "^21.0.0",
        "@types/sinon-chai": "^4.0.0",
        "chai": "^6.2.2",
        "chai-dom": "^1.12.1",
        "coveralls": "^3.1.1",
        "cross-env": "^10.1.0",
        "esbuild": "^0.27.2",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "jsdom": "^27.4.0",
        "jsdom-global": "^3.0.2",
        "mocha": "^11.7.5",
        "npm-run-all": "^4.1.5",
        "nyc": "^17.1.0",
        "prettier": "3.7.4",
        "prettier-plugin-organize-imports": "^4.3.0",
        "sinon": "^21.0.1",
        "sinon-chai": "^4.0.1",
        "tsx": "^4.21.0",
        "typedoc": "^0.28.15",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.52.0"
    },
    "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
